				div(class="  flex justify-center h-[86px] maxxl:h-[64] maxlg:h-[34px] w-max")
					img(  src="../assets/logo.png", alt="" class="  h-[86px] maxxl:h-[64] maxlg:h-[34px]")
			div( @click.away="isMobile? sideBar=false :''" x-show="sideBar" class="maxlg:fixed rtl:right-0 ltr:left-0 maxlg:top-0 maxlg:min-w-[280px] maxlg:pt-[60px] lg:flex items-center maxlg:bg-secondary maxlg:px-[16px] maxlg:h-screen maxlg:min-h-screen maxlg:text-white gap-[3%]")
				nav( class="flex justify-between gap-7 maxlg:flex-col maxlg:items-center min-w-max")

					a(href="/", target="_blank", rel="noopener noreferrer") 
