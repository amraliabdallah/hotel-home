					a(href="http://", target="_blank", rel="noopener noreferrer") 
						h5() About Us
					a(href="http://", target="_blank", rel="noopener noreferrer") 
						h5() Services  
					a(href="http://", target="_blank", rel="noopener noreferrer") 
						h5() Technology 
					a(href="http://", target="_blank", rel="noopener noreferrer") 
						h5()   Contact Us
				div( class="relative flex maxlg:flex-col justify-center items-center maxlg:w-full w-max gap-6 py-4 text-lg maxlg:mt-[40px]" x-data="{login:false}")
					div(class="w-[91px] h-[33px]")
						button(type="button" class="border-red-500 border rounded-full p-2 w-[91px] h-[33px] cent" x-on:click="login=true")  LogIn
